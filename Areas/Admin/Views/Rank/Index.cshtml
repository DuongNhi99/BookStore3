@model IEnumerable<BookStore.Models.Rank>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TenRank)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TenRank)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Ranking }) |
                @Html.ActionLink("Details", "Details", new { id = item.Ranking }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Ranking })
            </td>
        </tr>
    }

</table>

